<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ServletProject</display-name>
	<welcome-file-list>

		<welcome-file>index.jsp</welcome-file>

	</welcome-file-list>

	<servlet>
		<servlet-name>1</servlet-name>
		<servlet-class>com.project.Addservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>1</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>2</servlet-name>
		<servlet-class>com.project.Updateservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>2</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>3</servlet-name>
		<servlet-class>com.project.Deleteservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>3</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>4</servlet-name>
		<servlet-class>com.project.Getservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>4</servlet-name>
		<url-pattern>/get</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>5</servlet-name>
		<servlet-class>com.project.GetAllServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>5</servlet-name>
		<url-pattern>/getall</url-pattern>
	</servlet-mapping>
	
	

</web-app>